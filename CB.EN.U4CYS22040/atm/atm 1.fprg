                                                </else>
                                            </if>
                                        </then>
                                        <else>
                                            <output expression="&quot;wrong pin&quot;" newline="True"/>
                                        </else>
                                    </if>
                                    <output expression="c" newline="True"/>
                                </then>
                                <else>
                                    <if expression="n==4">
                                        <then>
                                            <output expression="&quot;enter your current pin&quot;" newline="True"/>
                                            <input variable="b"/>
