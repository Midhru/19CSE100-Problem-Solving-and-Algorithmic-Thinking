    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="a, b, c" type="Integer" array="False" size=""/>
            <declare name="e" type="Integer" array="False" size=""/>
            <declare name="d" type="String" array="False" size=""/>
            <declare name="n" type="Integer" array="False" size=""/>
            <declare name="m" type="Integer" array="False" size=""/>
            <declare name="l" type="Integer" array="False" size=""/>
            <declare name="x, y, z" type="Integer" array="False" size=""/>
            <output expression="&quot;WELCOME TO SMART BLOCK BANK&quot;" newline="True"/>
            <output expression="&quot;what you want to do&quot;" newline="True"/>
            <output expression="&quot;1.withdrawl&quot;" newline="True"/>
            <output expression="&quot;2.deposit&quot;" newline="True"/>
            <output expression="&quot;3.bank balknce&quot;" newline="True"/>
            <output expression="&quot;4.change pin&quot;" newline="True"/>
            <input variable="n"/>
            <if expression="n==1">
                <then>
                    <output expression="&quot;enter your account number&quot;" newline="True"/>
                    <input variable="a"/>
                    <if expression="a==456789">
                        <then>
                            <output expression="&quot;Your name:&quot;" newline="True"/>
